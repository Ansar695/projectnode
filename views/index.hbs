}
.freelancer{
	text-align: center;
	color: white;
	clip-path: polygon(0 0, 100% 0, 100% 100%, 75% 87%, 0% 100%);
	background: linear-gradient(rgba(0,0,0,0.9),rgba(0,0,0,0.9)), url("./Images/aboutme.jpg");
	padding-bottom: 8rem;
	background-size: cover;
	background-attachment: fixed;
	background-position: 100%;
	background-repeat: no-repeat;
}
.freelancer h1{
	margin-bottom: 1rem;
}
.freelancer p{
	margin-bottom: 2rem;
	color: #898989;
