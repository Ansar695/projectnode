.work{
	background: #F2F2F2;
	text-align: center;
	position: relative;
}
.work h1{
	margin-bottom: 1rem;
}
.work h1 span{
	color: var(--primary);
}
.work p{
	color: #898989;
	margin-bottom: 3rem;
}
.work .work-card-wrapper{
	display: grid;
	grid-template-columns: repeat(3, 1fr);
	grid-gap: 1.5rem;
	position: relative;
}
.work .work-card-wrapper .card{
	height: 260px;
	position: relative;
	border-radius: 5px;
}
.work .work-card-wrapper .card img{
	width: 100%;
	height: 100%;
	border-radius: 5px;
	object-fit: cover;
}
.work .work-card-wrapper .card .overlay{
	position: absolute;
	background: rgba(0,0,0,0.5);
	top: 100%;
	left: 100%;
	right: 100%;
	bottom: 100%;
	border-radius: 5px;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	transition: all .3s ease;
	overflow: hidden;
}
.work .work-card-wrapper .card .overlay .head-top{
	padding: .5rem 0rem;
	color: #F2F2F2!important;
	transform: translateX(0%);
	opacity: 1;
	animation: slideHead 1s alternate ease;
	display: none;
}
.work .work-card-wrapper .card:hover .head-top{
	display: block;
}
@keyframes slideHead{
	0%{transform: translateX(-150%);
	opacity: 0;
	}
	25%{
	transform: translateX(-150%);
	opacity: 0;
	}
	100%{transform: translateX(0);
	opacity: 1; 
	}
}
.work .work-card-wrapper .card .overlay .para-bottom{
	padding: .5rem 0rem;
	color: #F2F2F2!important;
	font-size: 14px!important;
	transform: translateY(0%);
	animation: slidePara 1.3s alternate ease;
	display: none;
}
.work .work-card-wrapper .card:hover .para-bottom{
	display: block;
}
@keyframes slidePara{
	0%{transform: translateY(400%);
	opacity: 0;
	}
	50%{
	transform: translateY(400%);
	opacity: 0;
	}
	100%{transform: translateY(0%);
	opacity: 1; 
	}
}
.work .work-card-wrapper .card:hover .overlay{
	background: rgba(0,0,0,0.5);
	bottom: 0;
	top: 0;
	left: 0;
	right: 0;
}
.work .work-card-wrapper .card .overlay i{
	color: white;
	font-size: 25px;
	margin-top: 2rem;
	padding: 20px;
	opacity: 0;
	transition: all .3s ease;
	cursor: pointer;
}
.work .work-card-wrapper .card:hover .overlay i{
	opacity: 1;
}
.work-del{
    text-decoration: none;
}
.work-del i{
    color: red!important;
    font-size: 15px!important;
    margin-bottom: .5rem;
}
#add-work{
