        :root{
	--primary: #3DCFD3;
}
*{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
	-webkit-font-smoothing: antialiased;
	font-family: 'poppins', sans-serif;
	font-weight: 500;
	
}
html{
	scroll-behavior: smooth;
}
body{
	position: relative;
}
ul{
	list-style: none;
}
a{
	text-decoration: none;
}
.social-sidebar{
	position: fixed;
	top: 38%;
	bottom: 38%;
	right: 0;
	width: 30px;
	height: 150px;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	z-index: 1111;
}
.social-sidebar a{
	text-decoration: none;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 21%;
	margin-top: 5px;
	border-top-left-radius: 3px;
	border-bottom-left-radius: 3px;
	background: var(--primary);
	transition: all .2s ease-in-out;
	color: white;
}
.social-sidebar a:hover{
	width: 100px;
	align-items: flex-start;
	padding-left: 6px;
	background: red;
}
.side-bar{
	position: fixed;
	top: 0;
	left: -100%;
	right: 100%;
	bottom: 0;
	background: white;
	box-shadow: 2px 6px 6px rgba(0,0,0,0.5);
	z-index: 10001;
}
.side-bar-active{
	left: 0;
	right: 50%;
	animation: all .3s ease-in-out;
}
@keyframes all{
	0%{
		left: -100%;
		right: 100%
	}
	100%{
		left: 0;
		right: 50%;
	}
}
.side-bar-img{
	width: 120px;
	height: 120px;
	border-radius: 50%;
	margin: auto;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.side-bar-img img{
	width: 100%;
	height: 100%;
	object-fit: cover;
	border-radius: 50%;
	margin-top: 2rem;
}
.side-bar h4{
	color: #333;
	margin-top: 2rem;
	letter-spacing: 1px;
	font-size: 18px;
	text-align: center;
	margin-bottom: 1rem;
}
.side-bar-links{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	padding: 2rem 3rem;
}
.side-bar-links a{
	text-decoration: none;
	color: #333;
	margin-top: 1rem;
	font-size: 18px;
	letter-spacing: .5px;
}

header{
